{
  "name": "vscode-pawi",
  "displayName": "Pawi Live Coding",
  "publisher": "pawijs",
  "description": "Live coding and visual composition in VSCode",
  "version": "0.0.0",
  "license": "MIT",
  "icon": "logo.png",
  "publishTo": "vscode",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pawijs/pawi.git"
  },
  "engines": {
    "vscode": "^1.54.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCustomEditor:vscode-pawi.treeEditor"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "customEditors": [
      {
        "viewType": "vscode-pawi.treeEditor",
        "displayName": "Tree Editor",
        "selector": [
          {
            "filenamePattern": "index.o.ts"
          }
        ],
        "priority": "default"
      }
    ],
    "commands": []
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "npm run build:view -- --watch & webpack --watch",
    "package": "npm run compile -- --mode production --devtool hidden-source-map",
    "test": "tsc -p ./",
    "build:watch": "webpack --config webpack.view-prod.js --watch",
    "build": "webpack --config webpack.view-prod.js",
    "devtools": "npx overmind-devtools",
    "start": "webpack serve --port 1236 --hot --inline --config webpack.view-dev.js"
  },
  "dependencies": {},
  "sideEffects": [
    "*.css"
  ],
  "devDependencies": {
    "@forten/build": "^1.0.0",
    "@forten/dragdrop": "^1.0.0",
    "@forten/preferences": "^1.0.0",
    "@forten/styled": "^1.0.0",
    "@forten/tree-type": "^1.0.0",
    "@forten/tree-view": "^1.0.0",
    "@forten/tree": "^1.0.0",
    "@types/node": "^14.14.31",
    "@types/vscode": "^1.54.0",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "css-loader": "^4.3.0",
    "file-loader": "^6.1.0",
    "html-webpack-plugin": "^5.2.0",
    "mini-css-extract-plugin": "^0.11.2",
    "pawi": "next",
    "ts-loader": "^8.0.17",
    "typescript": "^4.1.5",
    "vscode-test": "^1.5.0",
    "webpack-cli": "^4.5.0",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^5.7.3",
    "webpack": "^5.24.3"
  }
}