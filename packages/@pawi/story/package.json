{
  "name": "@pawi/story",
  "version": "0.0.0",
  "description": "Simple component story book.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pawijs/pawi.git"
  },
  "author": "Maia Taewana <maia@pawijs.org>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "build": "tsc -p setup/tsconfig.${NODE_ENV:=development}.json",
    "postbuild": "cp package.json ../../node_modules/@pawi/story",
    "clean": "rimraf .cache dist coverage ../../node_modules/@pawi/story",
    "prepare": "rimraf dist && cp -r ../../node_modules/@pawi/story/dist dist",
    "stories": "parcel serve --port 8888 --no-autoinstall src/stories/index.html",
    "test": "jest -c setup/jest.js",
    "test:watch": "jest -c setup/jest.js --watch --coverage false"
  },
  "dependencies": {
    "@pawi/build": "next",
    "@pawi/theme": "next",
    "jest-styled-components": "^7.0.3",
    "parcel": "^1.12.4"
  },
  "devDependencies": {}
}