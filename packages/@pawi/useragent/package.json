{
  "name": "@pawi/useragent",
  "version": "0.0.0",
  "description": "Handle keyboard useragent.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pawijs/pawi.git"
  },
  "author": "Maia Taewana <maia@pawijs.org>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "build": "tsc -p setup/tsconfig.${NODE_ENV:=development}.json",
    "clean": "rimraf .cache dist coverage ../../node_modules/@pawi/useragent",
    "postbuild": "cp package.json ../../node_modules/@pawi/useragent",
    "prepare": "rimraf dist && cp -r ../../node_modules/@pawi/useragent/dist dist",
    "stories": "parcel serve --port 8888 --no-autoinstall src/stories/index.html",
    "test:watch": "jest -c setup/jest.js --watch --coverage false",
    "test": "jest -c setup/jest.js"
  },
  "dependencies": {
    "@pawi/build": "next",
    "@pawi/hooks": "next",
    "@pawi/preferences": "next",
    "bowser": "^2.11.0",
    "workbox-window": "^5.1.4"
  },
  "devDependencies": {
    "@pawi/story": "next",
    "@pawi/styled": "next"
  }
}