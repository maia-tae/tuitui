{
	"name": "vscode-tuist",
	"displayName": "Tuist Live Coding",
	"description": "Live coding and visual composition in VSCode",
	"version": "0.0.1",
	"private": true,
	"engines": {
		"vscode": "^1.54.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:vscode-tuist.helloWorld",
		"onCommand:vscode-tuist.edit",
		"onCustomEditor:vscode-tuist.treeEditor"
	],
	"main": "./dist/extension.js",
	"contributes": {
		"customEditors": [
			{
				"viewType": "vscode-tuist.treeEditor",
				"displayName": "Tree Editor",
				"selector": [
					{
						"filenamePattern": "*.tui.json"
					}
				],
				"priority": "default"
			}
		],
		"commands": [
			{
				"command": "vscode-tuist.helloWorld",
				"title": "Hello World"
			},
			{
				"command": "vscode-tuist.edit",
				"title": "Open editor"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run package",
		"compile": "webpack",
		"watch": "webpack --watch",
		"package": "webpack --mode production --devtool hidden-source-map",
		"test-compile": "tsc -p ./",
		"test-watch": "tsc -watch -p ./",
		"pretest": "npm run test-compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"dependencies": {
		"@tuist/build": "next",
		"@tuist/locale": "next",
		"@tuist/editor": "next",
		"overmind": "25.0.2",
		"overmind-react": "26.0.2",
		"react-dom": "16.13.1",
		"react": "16.13.1"
	},
	"devDependencies": {
		"@types/vscode": "^1.54.0",
		"@types/node": "^12.11.7",
		"@typescript-eslint/eslint-plugin": "^4.14.1",
		"@typescript-eslint/parser": "^4.14.1",
		"typescript": "^4.1.3",
		"vscode-test": "^1.5.0",
		"ts-loader": "^8.0.14",
		"webpack": "^5.19.0",
		"webpack-cli": "^4.4.0"
	}
}