{
  "name": "@pawi/base",
  "version": "0.0.0",
  "description": "Official basic pawi blocks",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/pawijs/pawi.git"
  },
  "scripts": {
    "build": "tsc -p setup/tsconfig.${NODE_ENV:=development}.json",
    "postbuild": "cp package.json ../../node_modules/@pawi/base",
    "prepare": "rimraf dist && cp -r ../../node_modules/@pawi/base/dist dist",
    "clean": "rimraf .cache dist coverage ../../node_modules/@pawi/base"
  },
  "author": "Maia Taewana <maia@pawijs.org>",
  "license": "MIT",
  "dependencies": {
    "pawi": "next"
  },
  "devDependencies": {
    "@snowpack/plugin-typescript": "^1.2.1",
    "@types/node": "^14.14.31",
    "@types/simple-mock": "^0.8.1",
    "@types/webmidi": "^2.0.4",
    "snowpack": "^3.0.13",
    "snowpack-pawi": "next",
    "typescript": "^4.1.5"
  }
}