{
  "name": "@pawi/dragdrop",
  "version": "0.0.0",
  "description": "Drag and drop support.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pawijs/pawi.git"
  },
  "author": "Maia Taewana <maia@pawijs.org>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "build": "tsc -p setup/tsconfig.${NODE_ENV:=development}.json",
    "clean": "rimraf .cache dist coverage ../../node_modules/@pawi/dragdrop",
    "postbuild": "cp package.json ../../node_modules/@pawi/dragdrop",
    "prepare": "rimraf dist && cp -r ../../node_modules/@pawi/dragdrop/dist dist",
    "stories": "parcel serve --port 8888 --no-autoinstall src/stories/index.html",
    "test:watch": "jest -c setup/jest.js --watch --coverage false",
    "test": "jest -c setup/jest.js"
  },
  "dependencies": {
    "@pawi/build": "next",
    "@pawi/hooks": "next",
    "@pawi/theme": "next",
    "classnames": "^2.2.6"
  },
  "devDependencies": {
    "@pawi/story": "next"
  }
}